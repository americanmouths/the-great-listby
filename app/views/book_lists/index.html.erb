<h1>Your Current Book Lists</h1>
<% @booklists.each do |book_list| %>
  <h3>Title: <%= link_to book_list.title, book_list_path(book_list) %></h3>
  Books on this list:
  <% book_list.books.each do |book| %>
    <li><%= link_to book.title, book_path(book) %></li>
    Status: <%= book.book_features.find_by(book_id: book.id, book_list_id: book_list.id).status %><br><br>
  <% end %>
<% end %>
